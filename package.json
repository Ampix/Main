{
	"name": "ampix-main",
	"module": "src/main.ts",
	"type": "module",
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"scripts": {
		"dev:main": "bun run --watch src/main.ts",
		"dev:tw": "bunx tailwindcss -i ./src/main.css -o ./public/style.css --watch",
		"dev": "concurrently \"bun run dev:main\" \"bun run dev:tw\" \"bun run dev:build\"",
		"dev:build": "concurrently \"bun run build:*\"",
		"build:sckk": "bun build ./src/client/sckk/*.ts --outdir ./public/scripts/sckk --watch",
		"build:matek": "bun build ./src/client/matek/*.ts --outdir ./public/scripts/matek --watch",
		"build:nemet": "bun build ./src/client/nemet/*.ts --outdir ./public/scripts/nemet --watch",
		"build:user": "bun build ./src/client/user/*.ts --outdir ./public/scripts/user --watch",
		"build:vakterkep": "bun build ./src/client/user/*.ts --outdir ./public/scripts/vakterkep --watch",
		"start": "bun run src/main.ts"
	},
	"dependencies": {
		"@biomejs/biome": "^1.5.2",
		"@types/cookie-parser": "^1.4.6",
		"@types/express": "^4.17.21",
		"@types/node": "^20.11.5",
		"concurrently": "^8.2.2",
		"cookie-parser": "^1.4.6",
		"discord-oauth2": "^2.12.0",
		"express": "^4.18.2"
	}
}
